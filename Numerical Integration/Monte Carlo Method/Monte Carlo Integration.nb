Cell[TextData[{
 "The area of the rectangle therefore would be 2. For simplicity let us \
generate a mere 100 points and see what ratio ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"(", 
         FractionBox[
          StyleBox["n", "TI"], 
          StyleBox["N", "TI"]], ")"}], TraditionalForm], "errors" -> {}, 
     "input" -> "( \\frac{n}{N})", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "db5561e3-e235-4507-8375-e3413f8090a2"],
 " do we get. "
}], "Text",
 CellChangeTimes->{{3.950704984193264*^9, 3.9507050154996853`*^9}, 
   3.950705077597848*^9},ExpressionUUID->"dc7afddd-8522-47b2-b921-\
c17103768482"],

Cell[BoxData[
 RowBox[{
  RowBox[{"Do", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"x", "=", 
      RowBox[{"RandomReal", "[", 
       RowBox[{"{", 
        RowBox[{"0", ",", "2"}], "}"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"y", "=", 
      RowBox[{"RandomReal", "[", 
       RowBox[{"{", 
        RowBox[{"0", ",", "1"}], "}"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"y", "<", 
        RowBox[{"f", "[", "x", "]"}]}], ",", 
       RowBox[{"pointsUnderCurve", "++"}]}], "]"}]}], ",", 
    RowBox[{"{", "totalPoints", "}"}]}], "]"}], ";"}]], "Input",
 CellChangeTimes->{{3.950703660477805*^9, 3.9507036837499466`*^9}},
