 RowBox[{
  RowBox[{
   RowBox[{"f", "[", "x_", "]"}], ":=", 
   RowBox[{"1", "+", 
    RowBox[{
     RowBox[{"x", "^", "3"}], 
     RowBox[{"Exp", "[", 
      RowBox[{
       RowBox[{"-", "2"}], " ", "x"}], "]"}]}]}]}], 
  "\[IndentingNewLine]"}], "\n", 
 RowBox[{
  RowBox[{"xMin", "=", "0"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"xMax", "=", "2"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"yMin", "=", "1"}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"yMax", "=", "1.18"}], ";"}], "\[IndentingNewLine]"}], "\n", 
 RowBox[{"Show", "[", 
  RowBox[{
   RowBox[{"Plot", "[", 
    RowBox[{
     RowBox[{"f", "[", "x", "]"}], ",", 
