Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"f", "[", "x_", "]"}], ":=", 
   RowBox[{"Exp", "[", 
    RowBox[{
     RowBox[{"-", "2"}], " ", "x"}], "]"}]}], "\[IndentingNewLine]"}], "\n", 
 RowBox[{
  RowBox[{"xMin", "=", "0"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"xMax", "=", "2"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"yMin", "=", "0"}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"yMax", "=", "1"}], ";"}], "\n"}], "\[IndentingNewLine]", 
 RowBox[{"Show", "[", 
  RowBox[{
   RowBox[{"Plot", "[", 
    RowBox[{
     RowBox[{"f", "[", "x", "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"x", ",", "xMin", ",", "xMax"}], "}"}], ",", 
     RowBox[{"PlotStyle", "->", 
      RowBox[{"{", 
       RowBox[{"Thick", ",", 
        RowBox[{"Darker", "[", "Green", "]"}]}], "}"}]}]}], "]"}], ",", "\n", 
   RowBox[{"Graphics", "[", 
    RowBox[{"{", 
